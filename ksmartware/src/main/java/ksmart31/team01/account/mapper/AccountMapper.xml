<!DOCTYPE mapper
  PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
  "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="ksmart31.team01.account.mapper.AccountMapper">
	<!-- account 리스트 -->
	<resultMap type="ksmart31.team01.member.domain.Account" id="selectAccountList">
		<result column="account_code" property="accountCode"/>
		<result column="account_name" property="accountName"/>
		<result column="account_use" property="accountUse"/>
		<result column="account_date" property="accountDate"/>
		<result column="member_id" property="memberId"/>
	</resultMap>
	<select id="selectAccountList" resultType="ksmart31.team01.member.domain.Account" resultMap="selectAccountList">
		select
			account_code
			,account_name
			,account_use
			,account_date
			,member_id
		from account			
	</select>
</mapper>  